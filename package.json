{
  "scripts": {
    "docker-build": "docker build --force-rm --no-cache -t smartio/3pl-central .",
    "docker-run": "docker run -p 3000:80 -p 3001:3306 -p 3002:6379 -it --rm --name 3pl-central -v \"$PWD\":/usr/src/app -w /usr/src/app smartio/3pl-central",
    "docker-bash": "docker exec -i -t 3pl-central bash",
    "predocker-publish": "npm run docker-build",
    "docker-publish": "docker push smartio/3pl-central",
    "docker-dev": "docker build -t smartio/3pl-central . && npm run docker-run"
  }
}
