{
  "name": "3pl-central",
  "author": "Gabriel Bull",
  "version": "0.1.3",
  "description": "3PL Central API Wrapper",
  "main": "./lib/index.js",
  "keywords": [
    "3pl",
    "central",
    "api"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smart-io/3pl-central.git"
  },
  "dependencies": {
    "amqplib": "^0.4.1",
    "dotenv": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.12.0",
    "mongodb": "^2.1.16",
    "rxjs": "^5.0.0-beta.6",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "mocha": "^2.4.5"
  },
  "scripts": {
    "build": "./node_modules/.bin/babel ./src --out-dir ./lib",
    "docker-build": "docker build --force-rm --no-cache -t smartio/3pl-central .",
    "docker-run": "docker run -p 4369:4369 -p 5671:5671 -p 5672:5672 -p 25672:25672 -p 27017:27017 -p 15672:15672 -it --rm --name 3pl-central -v \"$PWD\":/usr/src/app -w /usr/src/app smartio/3pl-central",
    "docker-bash": "docker exec -i -t 3pl-central bash",
    "predocker-publish": "npm run docker-build",
    "docker-publish": "docker push smartio/3pl-central",
    "predocker-dev": "rm -f Dockerfile_dev && cp Dockerfile Dockerfile_dev && sed -i '.bak' 's/# DEV //g' Dockerfile_dev && rm -f Dockerfile_dev.bak",
    "docker-dev": "docker build -t smartio/3pl-central -f Dockerfile_dev . && npm run docker-run",
    "eslint": "./node_modules/.bin/eslint ./src ./test",
    "prebuild": "rm -rf lib && npm run eslint && npm run test",
    "prepublish": "npm run build",
    "serve": "./node_modules/.bin/babel-node src/server.js",
    "test": "./node_modules/.bin/mocha test"
  }
}
